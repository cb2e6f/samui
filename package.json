{
  "name": "loopy-browser",
  "version": "0.0.1",
  "description": "A performant scientific image visualization system.",
  "type": "module",
  "main": "src/electron.cjs",
  "productName": "Loopy Browser",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron",
    "build:electron": "electron-builder -mwl --config build.config.json",
    "build:svelte": "npm run fetch && svelte-kit build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "npm run dev:svelte",
    "dev:all": "concurrently -n=svelte,electron -c='#ff3e00',blue \"npm run dev:svelte\" \"npm run dev:electron\"",
    "dev:electron": "electron src/electron.cjs",
    "dev:svelte": "npm run fetch && svelte-kit dev",
    "fetch": "ts-node --esm getJson.ts",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. . && prettier-package-json --write ./package.json",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "start": "svelte-kit start",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "chart.js": "^3.7.1",
    "chartjs-plugin-datalabels": "^2.0.0",
    "colormap": "^2.3.2",
    "electron-context-menu": "^2.5.2",
    "electron-reloader": "^1.2.3",
    "electron-serve": "^1.1.0",
    "electron-window-state": "^5.0.3",
    "fzf": "^0.5.1",
    "lodash-es": "^4.17.21",
    "lru-cache": "^7.7.3",
    "ol": "^6.14.1",
    "pako": "^2.0.4",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@rgossiaux/svelte-headlessui": "^1.0.0-beta.12",
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "next",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/svelte": "^3.1.1",
    "@types/colormap": "^2.3.1",
    "@types/electron-window-state": "^2.0.34",
    "@types/lodash-es": "^4.17.6",
    "@types/lru-cache": "^7.6.1",
    "@types/pako": "^1.0.3",
    "@types/wicg-file-system-access": "^2020.9.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^27.5.1",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.6.0",
    "electron": "^12.2.3",
    "electron-builder": "^22.14.13",
    "electron-connect": "^0.6.3",
    "electron-packager": "^15.4.0",
    "electron-updater": "^4.6.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte": "^1.1.2",
    "eslint-plugin-svelte3": "^3.4.1",
    "node-fetch": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.6.2",
	"prettier-package-json": "^2.6.3",
    "prettier-plugin-svelte": "^2.7.0",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "sass": "^1.50.0",
    "svelte": "^3.47.0",
    "svelte-check": "^2.2.6",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  },
  "engines": {
    "npm": ">=7"
  },
  "browserslist": [
    "Chrome 100"
  ]
}
